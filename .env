# Environment variables for React Monitoring Bike App
# All variables MUST start with REACT_APP_ to be accessible in React components

# Backend API URL - Change this to your actual backend URL
# REACT_APP_BACKEND_URL=https://be.mobike-app.online
# REACT_APP_SOCKET_URL=https://be.mobike-app.online
REACT_APP_BACKEND_URL=http://localhost:5005
REACT_APP_SOCKET_URL=http://localhost:5005

# Optional: API key for authentication
REACT_APP_API_KEY=

# Optional: Control logging behavior
REACT_APP_DISABLE_LOGS=false
REACT_APP_DISABLE_REACT_WARNINGS=true

# Optional: App information
REACT_APP_VERSION=1.0.0
REACT_APP_TITLE=MoBike Monitoring
